;; misc sundries

(define (foo)
  (let (num-args (length %rest))
    (cond 
      ((= num-args 0) 100)
      ((= num-args 1) 101)
      (else 102))))
(foo)
=> 100

;; does it work for a macro too? no :( FIXME
(define-macro (bar)
  (let (num-args (length %rest))
    (cond 
      ((= num-args 0) 100)
      ((= num-args 1) 101)
      (else 102))))
(bar)
=> 100



