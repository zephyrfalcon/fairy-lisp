;; strings.test

(string->symbol "abc")
=> abc
(string->symbol "DEF")
=> def

(string-starts-with? "abc" "a")
=> true
(string-starts-with? "abc" "A")
=> false
(string-starts-with? "abc" "b")
=> false
(string-starts-with? "abc" "")  ;; empty prefix counts as true
=> true

(string-ends-with? "abc" "c")
=> true
(string-ends-with? "abc" "bc")
=> true
(string-ends-with? "abc" "b")
=> false
(string-ends-with? "foo/bar/baz.fs" ".fs")
=> true

(->string "abc")
=> "abc"
(->string "abc\ndef")
=> "abc\ndef"
(->string 123)
=> "123"
(->string #\ß)
=> "ß"

;; --- string-join ---

(%%string-join (list "a" "b" "c"))
=> "abc"
(%%string-join (list "a" "b" "c") " ")
=> "a b c"

(string-join '(a 1 b 2))
=> "a1b2"
(string-join '(a 1 b 2) ".")
=> "a.1.b.2"
(string-join (list "a" #\b 'c) " ")
=> "a b c"

(conc "Hello" #\, " world" #\!)
=> "Hello, world!"
(define x 42)
(conc "We ordered " x " hamburgers.")
=> "We ordered 42 hamburgers."

;; --- string-split ---

(string-split "a b c")
=> ("a" "b" "c")
(string-split "a   b  \n  c")
=> ("a" "b" "c")
(string-split "  a b  c  ")
=> ("a" "b" "c")
(string-split "foo:bar" ":")
=> ("foo" "bar")

