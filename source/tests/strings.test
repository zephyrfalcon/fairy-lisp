;; strings.test

(string->symbol "abc")
=> abc
(string->symbol "DEF")
=> def

(string-starts-with? "abc" "a")
=> true
(string-starts-with? "abc" "A")
=> false
(string-starts-with? "abc" "b")
=> false
(string-starts-with? "abc" "")  ;; empty prefix counts as true
=> true

(string-ends-with? "abc" "c")
=> true
(string-ends-with? "abc" "bc")
=> true
(string-ends-with? "abc" "b")
=> false
(string-ends-with? "foo/bar/baz.fs" ".fs")
=> true

(->string "abc")
=> "abc"
(->string "abc\ndef")
=> "abc\ndef"
(->string 123)
=> "123"
(->string #\ß)
=> "ß"

