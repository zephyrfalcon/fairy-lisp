;; test builtins and special forms

;; --- quote ---

'x
=> x
'3
=> 3
'()
=> ()
'(x y z)
=> (x y z)

;; --- define ---

(define x 42)
x
=> 42

;; --- lambda ---

(define inc 
  (lambda (x) (+ x 1)))
(inc 33)
=> 34

;; lambda with multiple expressions in body
(define foo
  (lambda (x y)
    x y))
(foo 9 10)
=> 10

;; --- do ---

(do 1 2 3)
=> 3
(do (+ 1 2) (+ 3 4))
=> 7

