(define m (make-module 'bah))
(type-name (type m))
=> module

(define m (make-module 'quux))
(env-set! (module-env m) 'zork 33)
(env-get (module-env m) 'zork)
=> 33

(define m (make-module 'quux))
(module-set! m 'baz 44)
(module-get m 'baz)
=> 44

(define m (make-module 'quux))
